{
  "name": "lezgi-numbers",
  "version": "1.0.1",
  "description": "Convert numbers to Lezgi numerals in your Node.js project or in browser using this ts/js library",
  "main": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test": "jest --collect-coverage --config jest.config.js",
    "start": "ts-node src/index.ts",
    "prebuild": "npm run test",
    "build": "tsc & webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LekiTech/lezgi-numbers"
  },
  "keywords": [
    "lezgi",
    "numbers",
    "converter",
    "natural language",
    "numerals",
    "tool",
    "лезги чIал"
  ],
  "author": "Kamran Tadzjibov",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "17.0.18",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
